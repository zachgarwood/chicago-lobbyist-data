<h3>{{model.name}}</h3>
<div>{{#if model.active}}Active{{else}}Inactive{{/if}}</div>
<div>
    <div><a href="tel:{{model.phone}}">{{model.phone}}</a></div>
    <div>
        {{model.address1}}{{#if model.address2}}, {{model.address2}}{{/if}}<br>
        {{model.city}}, {{model.state}} {{model.zip}}
    </div>
</div>
<ul>
    {{#each lobbyists as |lobbyist|}}
        <li>{{#link-to 'lobbyist.by-id' lobbyist}}{{lobbyist.lastName}}{{/link-to}}</li>
    {{/each}}
</ul>
